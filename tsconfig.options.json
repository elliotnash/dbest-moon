{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "isolatedModules": true,
    "composite": true,
    "incremental": true,
    "skipLibCheck": true
  }
}
